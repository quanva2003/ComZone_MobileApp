{
  "expo": {
    "name": "ComZone_MobileApp",
    "slug": "ComZone_MobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/hcn-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.wuanvan5076.ComZone_MobileApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Cho phép ứng dụng truy cập vào ảnh của bạn.",
          "saveToPhotos": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dbb9f64e-b056-4e06-b794-d4269f1af7eb"
      }
    }
  }
}
